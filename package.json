{
  "name": "alessio",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 3800",
    "build": "ng build && npm run build:ssr && npm run build:functions",
    "build:functions": "npm run --prefix functions build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run alessio:serve-ssr --port 4300",
    "serve:ssr": "node dist/alessio/server/main.js",
    "build:ssr": "ng build --prod && ng run alessio:server:production",
    "prerender": "ng run alessio:prerender",
    "emulators_firestore": "firebase emulators:start --only firestore --import=./data --export-on-exit",
    "deploy": "firebase deploy --only hosting"
  },
  "private": true,
